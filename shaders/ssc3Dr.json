            {
                "code": "void mainImage( out vec4 fragColor, in vec2 fragCoord )\n{\n    vec2 uv = fragCoord/iResolution.xy;\n    vec4 channelAColor = texture(iChannel0, uv);\n    vec4 textureBColor = texture(iChannel1, uv);\n\n    // Time varying pixel color\n    vec3 col = 0.5 + 0.5*cos(iTime * 2.0+uv.xyx+vec3(0,2,4));\n\n    float lerpFactor = (sin(iTime * 5.0) + 1.0) * 0.5;\n    // Output to screen\n    fragColor = mix(channelAColor, textureBColor, lerpFactor) * vec4(col,1.0);\n}",
