        "renderpass": [
            {
                "code": "void mainImage( out vec4 fragColor, in vec2 fragCoord )\n{\n    // Normalized pixel coordinates (from 0 to 1)\n    vec2 uv = fragCoord/iResolution.xy;\n    \n    float offsetX = 1.25; //オフセットX\n    float offsetY = 4.5; //オフセットY\n    float slideTime = iTime * 0.3; //切り替えのスピード\n    \n    \n    //テクスチャ1枚目\n    float mix1 = clamp(exp(2.0 * sin(slideTime)) - offsetY, 0.0 , 1.0);\n    vec4 tex1 = texture(iChannel0, uv) * mix1;\n\n    //テクスチャ2枚目\n    float mix2 = clamp(exp(2.0 * sin(slideTime - offsetX)) - offsetY, 0.0 , 1.0);\n    vec4 tex2 = texture(iChannel1, uv) * mix2;\n    \n    //テクスチャ3枚目\n    float mix3 = clamp(exp(2.0 * sin(slideTime - offsetX * 2.0)) - offsetY, 0.0 , 1.0);\n    vec4 tex3 = texture(iChannel2, uv) * mix3;\n    \n    //テクスチャ4枚目\n    float mix4 = clamp(exp(2.0 * sin(slideTime - offsetX* 3.0)) - offsetY, 0.0 , 1.0);\n    vec4 tex4 = texture(iChannel3, uv) * mix4;\n    \n    //テクスチャ5枚目\n    float mix5 = clamp(exp(2.0 * sin(slideTime - offsetX* 4.0)) - offsetY, 0.0 , 1.0);\n    vec4 tex5 = texture(iChannel2, uv) * mix5;\n      \n\n    vec4 col = tex1 + tex2 + tex3 + tex4 + tex5;\n    // Output to screen\n    fragColor = vec4(col);\n}",
