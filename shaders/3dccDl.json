        "renderpass": [
            {
                "code": "#define A(v)      clamp( r - R.y/32. * abs(v) , 0., 1. )                  //\n#define L(x,y)    ( a=b, b = vec2(x,y),                                    \\\n                    A( length( U-a - (b-a) * clamp(dot(U-a, b-a) / dot(b-a, b-a), 0., 1.) )) ) //\nvoid mainImage(out vec4 O,vec2 u) {                                 \n    vec2  R = iResolution.xy, a, B = vec2(.1,2), b,                       \n          V = ( u+u - R )  / R.y, U;\n    float T = mod(.3*iTime+2.,3.),\n          z = exp2(-T), r, l, i = 0., c = 0.,\n          t =  atan( 8. * sin(18.85*T) ) / 1.3 - .5;  // sin with faster change\n    V.x = abs(V.x);\n    for (; ++i<4.;  ) {                               // shape zoomed copies\n        U = V *= z*8.; z=1.;                          // scaling\n        c = 0.; U.y += t;                             // t: mouth animation\n        r = 1.5; b=B; c += L(1,-1)+L(2,2) + L(3,-.2)+L(3.9,1.6) + L(4.7,-.4)+L(5.5,.8) + L(6.2,-2)+L(6.8,.3);\n        r = .75; b=B; c += L(1, 4)+L(2,2) + L(3,3.5)+L(3.9,1.6) + L(4.7,2.5)+L(5.5,.8) + L(6.2, 2)+L(6.8,.3);\n        U.y = -U.y; U.y += t+t;          // above: top teeth below: bottom teeth\n        r = 1.5; b=B; c += L(.3,0)+L(1,2.2)+ L(2,-.4)+L(2.9,1.6) + L(3.9,-.2)+L(4.7,1.4) + L(5.5,.1)+L(5.8,1.2);\n        r = .75; b=B; c += L(.5,3)+L(1,2.2)+ L(2,  3)+L(2.9,1.6) + L(3.9,2.5)+L(4.7,1.4) + L(5.4, 2)+L(5.8,1.2);\n        l = i - T/3. -1.3; O += exp(-3.*l*l) * min(1.,c); // intensity fading small and large scale\n    }\n}",
