                "inputs": [],
                "name": "Common",
                "outputs": [],
                "type": "common"
            },
            {
                "code": "Main \n         Q = A(U);\n         vec2 V = unpackSnorm2x16(uint(Q.z));\n         \n         for (float x = -2.;x <= 2.;x++)\n         for (float y = -2.;y <= 2.;y++) {\n             vec2 u = vec2(x,y);\n             vec4 q = A(U+u);\n             vec4 m = B(U+u);\n             float l = length(u);\n             if (l>0.) {\n                 float r = exp(-.2*dot(u,u))/length(u);\n                Q.xy  -= .05*q.w*dot((mat2(m))*normalize(u),normalize(u))*u*r;\n                Q.xy  -= .05*q.w*(q.w-1.)*u*r;\n             }\n         }\n         Q.y -= .5/R.y;\n         \n         \n         \n        if (length(U-vec2(.5,.8)*R)<20.) Q = vec4(0,-.5,0,1);\n        \n    if (U.x < 10.||R.x-U.x < 10.||R.y-U.y < 10.||U.y< 10.) Q.xy *= 0.;\n        \n        Q.z = float(packSnorm2x16(V));\n    \n\n\n}",
