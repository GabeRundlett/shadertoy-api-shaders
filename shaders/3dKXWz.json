            },
            {
                "code": "void mainImage( out vec4 Q, in vec2 U )\n{\n    if (U.x>N||U.y>1.) discard;\n    Q = A(U);\n    vec4 f = vec4(0);\n    for (float i = 0.; i < N; i++){\n        f += B(vec2(U.x,i));\n    }\n    vec4\n        n = C(Q.xy+vec2(0,1)),\n        e = C(Q.xy+vec2(1,0)),\n        s = C(Q.xy-vec2(0,1)),\n        w = C(Q.xy-vec2(1,0));\n    f.xy = f.xy/f.w-3.*vec2(e.z-w.z,n.z-s.z);\n    Q.zw = Q.zw + f.xy;\n    Q.xy += Q.zw+.5*f.xy;\n    Q.w -= 1e-2;\n    if (Q.x>R.x-10.) {Q.x = R.x-10.;Q.z=-abs(Q.z);Q.w*=.9;}\n    if (Q.x<10.0) {Q.x = 10.0;Q.z=+abs(Q.z);Q.w*=.9;}\n    if (Q.y>R.y-10.) {Q.y = R.y-10.;Q.w=-abs(Q.w);Q.z*=.9;}\n    if (Q.y<10.0) {Q.y = 10.0;Q.w=+abs(Q.w);Q.z*=.9;}\n    \n    if (iFrame < 1||iMouse.z>0.) {\n    \tQ.xy = U.xx/N*vec2(1,10.*iTime+480./230.*N);\n        Q.xy = R*(.5+.125*Q.x*R.x/R*vec2(cos(Q.y),sin(Q.y)));\n        Q.zw = vec2(0);\n    }\n}",
                "description": "",
