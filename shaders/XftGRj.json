            {
                "code": "Main {\n    U = floor(U);\n    vec2 u = mod(U,M);\n    if (abs(u.x-.5*M)>.1||abs(u.y-.5*M)>.1) discard;\n    Q = B(U);\n    vec4 q = A(U);\n    float cQ = sign(hash(U.xyxy).x*2.-1.);\n    vec2 f = vec2(0,-.1/R.y);\n    float p = 0.;\n    for (float x = -2.; x<=2.;x++)\n    for (float y = -2.; y<=2.;y++) if (abs(x)>1.||abs(y)>1.) {\n        vec4 c = D(U+vec2(x,y));\n        if (c.z>5.&&c.w>5.&&c.x>5.&&c.y>5.) {\n            vec4 a = A(c.xy);\n            vec4 b = A(c.zw);\n            float cb = sign(hash(c.zwzw).x*2.-1.);\n            {\n              vec2 r = b.xy-q.xy;\n              float l = length(r);\n              float s = line(q.xy+vec2(x,y),a.xy,b.xy);\n              if (l > 0. && s < .5) {\n                  if (a.x>10.&&b.x>10.&&a.y>10.&&b.y>10.)\n                  p += .03*dot(b.zw-q.zw,normalize(r))/l/l;\n              }\n            }\n        }\n    }\n    \n    Q += p - .01*Q;\n    \n    if (iFrame < 1) {\n        Q = vec4(0);\n    }\n    \n\n}",
