            {
                "code": "Main {\n    \n    Q = A(U);\n    vec4 n = A(U+vec2(0,1));\n    vec4 e = A(U+vec2(1,0));\n    vec4 s = A(U-vec2(0,1));\n    vec4 w = A(U-vec2(1,0));\n    vec4 b = B(U);\n    vec4 C  =  vec4(sign(b.zwzw-.5*R.xyxy));//C(B(U).zw)*2.-1.;\n    Q.xy -= .5*vec2(e.z-w.z,n.z-s.z);\n    Q.y -= .1/R.y*C.z;\n    Q.x -= .1/R.y*C.w;\n    for (float x = -4.; x <= 4.; x++)\n    for (float y = -4.; y <= 4.; y++)\n    {\n        vec2 u = vec2(x,y);\n        vec4 q = B(b.xy+u);\n        vec4 c = vec4(sign(q.zwzw-.5*R.xyxy));//C(q.zw)*2.-1.;\n        vec2 r = (b.xy-q.xy);\n        float l = length(r);\n        if (l>=1.&&length(U+u-q.xy)<.75)\n        Q.xy -=.0003*r/l/l*(sign(c.z)*sign(C.z)+sign(c.w)*sign(C.w));\n    }\n    if (U.x < 5.||R.x-U.x<5.||U.y < 5.||R.y-U.y<5.)Q.xy *= 0.;\n    \n}",
