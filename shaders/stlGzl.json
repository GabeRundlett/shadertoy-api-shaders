        "renderpass": [
            {
                "code": "// Fork of \"Colors!\" by wyatt. https://shadertoy.com/view/7dX3z7\n// 2021-06-02 16:58:24\n\n// Fork of \"Transport Dynamics II\" by wyatt. https://shadertoy.com/view/sdl3RN\n// 2021-03-14 01:41:52\n\n// Display ðŸŒµ\nMain \n{\n    vec4 f = A(U), c = B(U);\n    Q = c*min(f.w,1.3);\n    Q = ((.5-.5*cos(5.*c.x+c.y*vec4(1.,2,3,4))))*atan(f.wwww);\n    Q += f.z*f.w*vec4(1,.5,0,1);\n}",
