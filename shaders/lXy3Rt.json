        "renderpass": [
            {
                "code": "\n\nfloat period = 3.0;\n\nvec4 red = vec4(1.0, 0.0, 0.0, 0.0);\nvec4 green = vec4(0.0, 1.0, 0.0, 0.0);\nvec4 blue = vec4(0.0, 0.0, 1.0, 0.0);\nvec4 white = vec4(1.0, 1.0, 1.0, 0.0);\nvec4 yellow = vec4(1.0, 1.0, 0.0, 0.0);\n\nvoid mainImage( out vec4 fragColor, in vec2 fragCoord )\n{\n    vec2 uv = fragCoord/iResolution.xy * 2.0;\n    \n    \n    float fade = max(sin(iTime), 0.0);\n    \n    vec4 color;\n    \n    if(uv.x < 1.0)\n    {\n        if(uv.y < 1.0)\n        {\n            color = mix(texture(iChannel2, fract(uv)), blue, fade);\n        }\n        else\n        {\n            color = mix(texture(iChannel0, fract(uv)), red, fade);\n        \n        }\n    }\n    else\n    {\n        if(uv.y < 1.0)\n        {\n            color = mix(texture(iChannel3, fract(uv)), yellow, fade);\n        }\n        else\n        {\n            color = mix(texture(iChannel1, fract(uv)), green, fade);\n        \n        }\n    }\n    \n    \n    \n    fragColor = color;\n    vec4 tex = texture(iChannel0, uv * 0.5);\n    fragColor = tex;\n    \n}",
