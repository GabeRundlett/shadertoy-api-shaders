            "viewed": 874
        },
        "renderpass": [
            {
                "code": "// To avoid the OpenGL array bug as in https://www.shadertoy.com/view/ls2BRt \n// (investigated here https://www.shadertoy.com/view/4sBBWy) \n// + for performance, store 3D array in BufA with band gard to avoid modulo-leaks.\n// Then xy interpolation is provided by hardware, only z still to be done.\n// Bunny data from SebH https://www.shadertoy.com/view/MdlyDs\n\n// Here: adding a guard band to https://www.shadertoy.com/view/MsBfD3\n// see also with precomputed gradients: https://www.shadertoy.com/view/MsSfWc\n\n#define voxelB(x,y,z) ( min(x,min(y,z)) >= 0. && max(x,max(y,z)) < 32.    \\\n            ? texture(iChannel0, (1.5+vec2(x,y)+33.*vec2(int(z)%8,int(z)/8))/iResolution.xy ).r \\\n            : 0. )   \n\n#define voxel(x,y,z) mix( voxelB(x,y, z), voxelB(x,y, z+1.), fract(z) )\n\n#define rot(a) mat2(cos(a),-sin(a),sin(a),cos(a))\n\n\nvoid mainImage( out vec4 O,vec2 V )\n{\n    V = 1.7* ( (V+V - iResolution.xy) / iResolution.y);\n    O -= O; \n    \n    for (float i=-1.4; i<1.; i+=.5/16.) {  \n        vec3 P = vec3( V, i); \n        P.yz *= rot(.78);\n        P.xz *= rot(-iTime);\n        vec3 A = abs(P);  if (max(A.x,max(A.y,A.z))>1.) continue;  // optimization\n        P = 16.*(P+1.);\n        O += (1.-O.a) * voxel( P.x , P.z, P.y  )\n                      * vec4( vec3(.5-.5*i), 1) ;\n    }\n    O += (1.-O.a) * vec4(.1,.3,0,1);\n}",
