            },
            {
                "code": "// Translate Particles ðŸš—\nMain {\n    Q = vec4(0);\n    vec4 QB = vec4(0);\n    for (float x=-3.;x<=3.;x++)\n    for (float y=-3.;y<=3.;y++)\n    {\n          vec2 xy = vec2(x,y);\n          vec4 b = B(U+xy);\n          if (b.w>0.) {\n              vec4 a = A(U+xy);\n              \n              vec4 _1 = a, _2 = a;\n              mat2 m = ei(6.2*hash(vec3(U+xy,iTime)).x);\n              if (b.w>.1) {\n                  vec2 v = vec2(0,Z)*m;\n                  _2.xy -= v;\n                  _1.xy += v;\n              }\n              if (_1.x<U.x+.5&&_1.x>=U.x-.5&&\n                  _1.y<U.y+.5&&_1.y>=U.y-.5){\n                  Q += .5*b.w*_1;\n                  QB.w += .5*b.w; \n              }\n              if (_2.x<U.x+.5&&_2.x>=U.x-.5&&\n                  _2.y<U.y+.5&&_2.y>=U.y-.5){\n                  Q += .5*b.w*_2;\n                  QB.w += .5*b.w;\n              }\n         }\n    }\n    if (QB.w >0.) Q /= QB.w;\n}",
