            {
                "code": "void X (inout vec4 Q, vec2 U, vec2 r) {\n\tvec4 n = A(U+r);\n    if (length(U-n.xy)<length(U-Q.xy)) Q = n;\n}\nMain\n{\n    Q = A(U);\n    X(Q,U,vec2(1,0));\n    X(Q,U,vec2(0,1));\n    X(Q,U,-vec2(1,0));\n    X(Q,U,-vec2(0,1));\n    X(Q,U,vec2(3,0));\n    X(Q,U,vec2(0,3));\n    X(Q,U,-vec2(3,0));\n    X(Q,U,-vec2(0,3));\n    \n    Q.zw = mix(Q.zw,C(Q.xy).zw,.01);\n    Q.w -= 5e-4;\n    Q.zw += B(Q.xy).xy;\n    Q.xy += Q.zw;\n    \n    if (Q.x<1.) {Q.x = 1.; Q.z *= -1.;}\n    if (Q.y<1.) {Q.y = 1.; Q.w *= -1.;}\n    if (R.x-Q.x<1.) {Q.x = R.x-1.; Q.z *= -1.;}\n    if (R.y-Q.y<1.) {Q.y = R.y-1.; Q.w *= -1.;}\n    \n    if (iMouse.z>0.) {\n    \tif (length(U-iMouse.xy)<length(U-Q.xy))\n            Q.xy = iMouse.xy;\n    }\n    if (iFrame < 1 && length (U-0.5*R) < 30.) Q.xy = U;\n}",
