            },
            {
                "code": "//Sort Graph ðŸ•¸\nMain\n{\n    Q = B(U);\n    Q.xy = A(U).xy;\n    for (float x=-2.;x<=2.;x++)\n    for (float y=-2.;y<=2.;y++)\n    {\n        vec2 u = vec2(x,y);\n        vec4\n            aa= A(U+u),\n            b = B(U+u),\n            a = vec4(A(b.xy).xy,A(b.zw).xy),\n            q = vec4(A(Q.xy).xy,A(Q.zw).xy);\n        if (cmp(U,q.xy,aa.xy,q.xy,q.zw))\n        {\n            Q.zw = q.zw = aa.xy;\n        } else\n        if (cmp(U,a.xy,a.zw,q.xy,q.zw))\n        {\n            q = a;\n            Q = b;\n        } else\n        if (cmp(U,q.xy,a.xy,q.xy,q.zw))\n        {\n            q.zw = a.xy;\n            Q.zw = a.zw;\n        }\n    }\n    Q.xy = A(Q.xy).xy;\n    Q.zw = A(Q.zw).xy;\n    if (length(U-Q.zw)<length(U-Q.xy))\n        Q = Q.zwxy;\n    if (iFrame<1) {\n        Q = vec4(U,U+1.);\n    }\n}",
