        "renderpass": [
            {
                "code": "// by Nikos Papadopoulos, 4rknova / 2017\n// WTFPL\n\n#define THRESHOLD (0.5)\n\n#define LUMINANCE(x) (dot(x, vec3(.2126, .7152, .0722)))\n\nvec3 dither(vec3 col)\n{\n    // A basic two-level algorithm for image halftoning.\n    // Choose a certain constant value (eg. the average image\n    // luminance value) and use it as a global threshold to\n    // decide whether a pixel should be quantized to 0 or 1.\n    return vec3(LUMINANCE(col) > THRESHOLD ? 1. : 0.);\n}\n\nvoid mainImage(out vec4 fragColor, in vec2 fragCoord) {\n    vec3 col = texture(iChannel0, fragCoord.xy / iResolution.xy).rgb;\n    fragColor = vec4(dither(col),1);\n}",
